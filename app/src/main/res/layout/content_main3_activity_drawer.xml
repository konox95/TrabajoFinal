<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main3_activity_drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="konox.actividad3.Main3ActivityDrawer"
    tools:showIn="@layout/app_bar_main3_activity_drawer">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="konox.actividad3.MapaFragment"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/frMapa" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="konox.actividad3.NuevoSpotFragment"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/frSpotNuevo" />

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="konox.actividad3.PerfilFragment"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/frPerfil"/>

    <fragment
        android:id="@+id/fragment_pin"
        android:name="konox.actividad3.pinFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" />

</RelativeLayout>
